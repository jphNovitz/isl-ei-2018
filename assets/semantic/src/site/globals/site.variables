@pageBackground    : rgba(252, 226, 209, .3);
@fontSize          : 16px;
@headerFont        : 'Cuprum', sans-serif;
@pageFont          : 'Roboto', sans-serif;
@importGoogleFonts : true;
@googleFontName    : 'Cuprum';
@googleFontSizes   : '400,700,400italic,700italic';
@googleFontFamily  : '@{googleFontName}:@{googleFontSizes}';