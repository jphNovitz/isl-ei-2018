{% extends '_skeletons/_card.html.twig' %}
{% set object = provider %}
{% if object.image is not empty %}
    {% block card_image %} {{ asset('images/providers/'~object.image.imageName) }}{% endblock %}
{% endif %}

{% block card_rest %}
    <p>{{ object.email }}
    <br /> {{ object.streetname }} {{ object.streetnr }}
</p>
{% endblock %}