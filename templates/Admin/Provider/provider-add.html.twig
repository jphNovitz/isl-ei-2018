{% extends '_skeletons/_forms.html.twig' %}

{% block title %}Ajout d'un fournisseur{% endblock %}
    {% block form_details %}
        <div class="form-row">
            <label for="form_name">Nom</label>
            {{ form_widget(form.name) }}
        </div>
        <div class="form-row">
            <label for="form_streetName">Rue</label>
            {{ form_widget(form.streetName) }}
        </div>
        <div class="form-row">
            <label for="form_streetNr">No</label>
            {{ form_widget(form.streetNr) }}
        </div>
        <div class="form-row">
            <label for="form_email">Email</label>
            {{ form_widget(form.email) }}
        </div>
    {% endblock %}
